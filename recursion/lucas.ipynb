        "def lucas(n):\n",
        "    if n == 0:\n",
        "        return 2\n",
        "    elif n == 1:\n",
        "        return 1\n",
        "    else:\n",
        "        return lucas(n - 1) + lucas(n - 2)\n",
        "\n",
        "\n",
        "print(lucas(10))"
      ],
