      "source": [
        "def tri(n):\n",
        "    if n == 0:\n",
        "        return 0\n",
        "    elif n == 1:\n",
        "        return 0\n",
        "    elif n == 2:\n",
        "        return 1\n",
        "    else:\n",
        "        return tri(n - 1) + tri(n - 2) + tri(n - 3)\n",
        "\n",
        "\n",
        "print(tri(10))"
      ],
