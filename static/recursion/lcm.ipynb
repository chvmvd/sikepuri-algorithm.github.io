      "source": [
        "def gcd(a, b):\n",
        "    if b == 0:\n",
        "        return a\n",
        "    else:\n",
        "        return gcd(b, a % b)\n",
        "\n",
        "\n",
        "def lcm(a, b):\n",
        "    return a * b / gcd(a, b)\n",
        "\n",
        "\n",
        "print(lcm(30, 18))"
      ],
