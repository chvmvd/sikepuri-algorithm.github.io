    "def sum_val(scores):\n",
    "    s = 0\n",
    "    for i in range(len(scores)):\n",
    "        s += scores[i]\n",
    "    return s\n",
    "\n",
    "\n",
    "A_SCORES = [83, 75, 32]\n",
    "B_SCORES = [73, 53, 84]\n",
    "C_SCORES = [63, 48, 64]\n",
    "\n",
    "print(sum_val(A_SCORES) + sum_val(B_SCORES) + sum_val(C_SCORES))"
   ]
  }
