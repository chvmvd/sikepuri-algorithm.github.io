    "def draw_cell(x, y):\n",
    "    image = ita.array.make2d(100, 100)\n",
    "    y = len(image) - 1 - y\n",
    "    image[y][x] = 1\n",
    "    return image\n",
    "\n",
    "\n",
    "image = draw_cell(10, 10)\n",
    "ita.plot.image_show(image)"
