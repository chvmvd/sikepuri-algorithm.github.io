    "def calc_sum_square(n):\n",
    "    s = 0\n",
    "    for i in range(1, n + 1):\n",
    "        for j in range(1, n + 1):\n",
    "            s += i * j\n",
    "    return s\n",
    "\n",
    "\n",
    "print(calc_sum_square(10))"
